{
  "name": "timesimp",
  "version": "1.0.1",
  "description": "Simple sans-io timesync client and server",
  "repository": "https://github.com/passcod/timesimp",
  "author": "FÃ©lix Saparelli <felix@passcod.name>",
  "license": "Apache-2.0 OR MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "timesimp",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "armv7-unknown-linux-musleabihf",
        "x86_64-unknown-linux-musl",
        "armv7-linux-androideabi",
        "universal-apple-darwin",
        "riscv64gc-unknown-linux-gnu"
      ]
    }
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "@types/node": "^22.13.13",
    "typedoc": "^0.28.1"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --js-package-name @passcod/timesimp",
    "build:debug": "napi build --platform",
    "docs": "typedoc index.d.ts",
    "prepublishOnly": "napi prepublish -t npm && sed -E -i 's|^    \"timesimp-|    \"@passcod/timesimp-|' package.json",
    "test": "node test/Timesimp.spec.mjs",
    "universal": "napi universal",
    "version": "napi version"
  },
  "optionalDependencies": {
    "@passcod/timesimp-win32-x64-msvc": "1.0.1",
    "@passcod/timesimp-darwin-x64": "1.0.1",
    "@passcod/timesimp-linux-x64-gnu": "1.0.1",
    "@passcod/timesimp-darwin-arm64": "1.0.1",
    "@passcod/timesimp-android-arm64": "1.0.1",
    "@passcod/timesimp-linux-arm64-gnu": "1.0.1",
    "@passcod/timesimp-linux-arm64-musl": "1.0.1",
    "@passcod/timesimp-win32-arm64-msvc": "1.0.1",
    "@passcod/timesimp-linux-arm-gnueabihf": "1.0.1",
    "@passcod/timesimp-linux-arm-musleabihf": "1.0.1",
    "@passcod/timesimp-linux-x64-musl": "1.0.1",
    "@passcod/timesimp-android-arm-eabi": "1.0.1",
    "@passcod/timesimp-darwin-universal": "1.0.1",
    "@passcod/timesimp-linux-riscv64-gnu": "1.0.1"
  }
}